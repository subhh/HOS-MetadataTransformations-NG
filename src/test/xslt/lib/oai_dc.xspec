<description stylesheet="../../../main/xslt/lift.xsl"
             xmlns="http://www.jenitennison.com/xslt/xspec"
             xmlns:dc="http://purl.org/dc/elements/1.1/"
             xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">

  <scenario label="When lifting an OAI DublinCore record with a DOI-URL">
    <context xmlns:dc="http://purl.org/dc/elements/1.1/">
      <dc:identifier>https://doi.org/12345.5678</dc:identifier>
    </context>
    <expect label="create a properly typed DOI identifier">
      <dc:identifier rdf:datatype="https://openscience.hamburg.de/vocab/datatype#DOI">12345.5678</dc:identifier>
    </expect>
  </scenario>
</description>
